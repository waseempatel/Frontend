let a = 10, b = 5;

console.log(a + b);    // Arithmetic: 15
console.log(a > b);    // Comparison: true
console.log(a === 10); // Strict equality: true

a += 5;                // Assignment: a = 15
console.log(a);

let isAdult = true;
let hasID = false;
console.log(isAdult && hasID); // Logical: false

// Ternary
let grade = 90;
let result = (grade >= 50) ? "Pass" : "Fail";
console.log(result); // Pass

// Type check
console.log(typeof result); // string
